{
  "name": "topaz-console",
  "private": true,
  "version": "0.1.2",
  "type": "module",
  "scripts": {
    "build": "tsc -b && vite build",
    "tsc": "tsc -b --noEmit --pretty",
    "detect-unused-exports": "knip --include exports,types,nsExports,nsTypes,duplicates,unlisted,classMembers,enumMembers",
